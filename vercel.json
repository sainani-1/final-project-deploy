
{
  "version": 2,
  "outputDirectory": "career-assesment-tool/dist",
  "builds": [
    {
      "src": "career-assesment-tool/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "career-assesment-tool/dist" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "career-assesment-tool/dist/$1" }
  ]
}
